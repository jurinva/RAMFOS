; Стандартная точка входа C84E
; Очистка всего экрана с отключением служебных строк
;
; Сохраняет все регистры
;
; 2013-11-01 Дизассемблировано vinxru
;----------------------------------------------------------------------------                   

fullClearScreen:
		push	psw

		; Эта переменная включает очистку всего экрана
		mvi	a, 1Fh
		sta	v_dblClear

		; Печать символа 1Fh и освобождение стека
		; call	printCharA
		; pop	psw
		; ret		
		jmp	printCharA_popret